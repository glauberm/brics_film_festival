{
  "files.exclude": {
    "**/.git": true,
    "gatsby/node_modules": true,
    "gatsby/.cache": true,
    "gatsby/public": true,
    "wordpress/vendor": true
  },
  "[js]": {
    "editor.tabSize": 2,
  },
  "[php]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
  },
  "phpcs.executablePath": "wordpress/vendor/bin/phpcs"
}
